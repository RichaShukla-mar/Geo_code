	</xsd:complexType>
	<xsd:complexType name="CountryStructure">
		<xsd:annotation>
			<xsd:documentation>A country. Holds attributes like country name, ISO
				code and ISO number.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="DefinedAreaStructure">
				<xsd:sequence>
					<xsd:element name="IsPostalCodeMandatoryInd" type="AMM:BooleanType"
						minOccurs="0" maxOccurs="1">
						<xsd:annotation>
							<xsd:documentation>A yes/no indicator of a 'Country' for the term
								'Is Postal Code Mandatory Ind'.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="IsPostalCodePredefinedInd" type="AMM:BooleanType"
						minOccurs="0" maxOccurs="1">
						<xsd:annotation>
							<xsd:documentation>A yes/no indicator of a 'Country' for the term
								'Is Postal Code Predefined Ind'.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="IsPostalCodeUniqueInCountryInd"
						type="AMM:BooleanType" minOccurs="0" maxOccurs="1">
						<xsd:annotation>
							<xsd:documentation>A yes/no indicator of a 'Country' for the term
								'Is Postal Code Unique In Country Ind'.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="IsCapitalCityMandatoryInd" type="AMM:BooleanType"
						minOccurs="0" maxOccurs="1">
						<xsd:annotation>
							<xsd:documentation>A yes/no indicator of a 'Country' for the term
								'Is Capital City Mandatory Ind'.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="ISOCountry" type="ISOCountryStructure"
						minOccurs="1" maxOccurs="1" />
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="DefinedAreaStructure">
		<xsd:annotation>
			<xsd:documentation>The supertype entity to all geographic and other
				kinds of areas such as Business Defined Areas (which still have a
				geographic aspect to their definition). Some areas are 'point', have
				specific geographical coordinates (e.g. the "city Centre" point)
				while other have a geographic boundary (defined perimeter) enclosing
				an area such as a Continent, Country, City or Site.
			</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="DefinedAreaId" type="AMM:Integer13Type"
				minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>Surrogate key for the defined area
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="ResponsibleOrganisationUnitId" type="AMM:Integer13Type"
				minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>A surrogate key that can uniquely identify an
						entry in 'Defined Area'.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="TimeZoneId" type="AMM:Integer13Type"
				minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>A surrogate key that can uniquely identify an
						entry in 'Defined Area'.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="DefinedAreaName" type="AMM:String100Type"
				minOccurs="0" maxOccurs="1" />
			<xsd:element name="DefinedAreaDescription" type="AMM:String250Type"
				minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>Descriptive text of an entry in 'Defined Area'
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="ValidFromDt" type="AMM:DateType"
				minOccurs="0" maxOccurs="1" />
			<xsd:element name="ValidThruDt" type="AMM:DateType"
				minOccurs="0" maxOccurs="1" />
			<xsd:element name="DaylightSavingsTimeId" type="AMM:Integer13Type"
				minOccurs="0" maxOccurs="1" />
			<xsd:element name="DefinedAreaCd" type="AMM:String50NonNullType"
				minOccurs="0" maxOccurs="1" />
			<xsd:element name="GeoIDs" type="GeoIDsStructure"
				minOccurs="0" maxOccurs="1" />	
			<xsd:element name="GeoCd" type="AlternativeCodeStructure"
				minOccurs="0" maxOccurs="1" />		
		</xsd:sequence>
	</xsd:complexType>
	
	<xsd:complexType name="GeoIDsStructure">
		<xsd:sequence>
			<xsd:element name="GeoID" type="AlternativeCodeStructure"
				minOccurs="1" maxOccurs="unbounded" />
		</xsd:sequence>			
	</xsd:complexType>
	


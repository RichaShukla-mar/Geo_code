         <xsd:element name="AreaCapacityMeasure"
                      type="AMM:Decimal18d4Type"
                      minOccurs="0"
                      maxOccurs="1"/>
         <xsd:element name="BerthCountNumber"
                      type="AMM:String50NonNullType"
                      minOccurs="0"
                      maxOccurs="1"/>
         <xsd:element name="BerthTotalLengthMeasure"
                      type="AMM:Decimal18d4Type"
                      minOccurs="0"
                      maxOccurs="1"/>
         <xsd:element name="DraftMaximumAlongsideMeasure"

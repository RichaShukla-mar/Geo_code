      </xsd:complexType>
      <xsd:complexType name="CraneStructure">
      <xsd:annotation>
         <xsd:documentation>A crane is a type of machine used for lifting, generally equipped with a hoist (device) or winder wire ropes or chains and sheaves, that can be used both to lift and lower materials and to move them horizontally.</xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="CraneIdentifier"
                      type="AMM:Integer13Type"
                      minOccurs="0"
                      maxOccurs="1"/>
         <xsd:element name="CraneTypeCode"
                      type="AMM:String50NonNullType"
                      minOccurs="0"
                      maxOccurs="1"/>
         <xsd:element name="LengthUnitIdentifier"
                      type="AMM:Integer13Type"
                      minOccurs="0"
                      maxOccurs="1"/>
         <xsd:element name="MassUnitIdentifier"
                      type="AMM:Integer13Type"
                      minOccurs="0"
                      maxOccurs="1"/>
         <xsd:element name="WeightCapacityMeasure"
                      type="UnitOfMeasurementStructure"
                      minOccurs="0"
                      maxOccurs="1">
            <xsd:annotation>
               <xsd:documentation>The maximum weight that the crane can lift.</xsd:documentation>
            </xsd:annotation>
         </xsd:element>             
       </xsd:sequence>
   </xsd:complexType>
   <xsd:complexType name="CranesStructure">

   		<xsd:element name="Crane" type="CraneStructure" minOccurs="1" maxOccurs="unbounded"/>
   	</xsd:sequence>
    </xsd:complexType>
     <xsd:complexType name="UnitOfMeasurementStructure">
      <xsd:annotation>
         <xsd:documentation>The metric by which a Measured Property is measured. Example instances are: 
 
    Second / Hour / Day / Year,
    Micron / Centimetre / Metre / Kilometer / Nautical Mile,
    Inch / Foot / Yard / Fathom / Mile / Nautical Mile,
    Litre,
    US Gallon,
    Gram / Kilogram / Tonne,
    Pound / Ton,
    Knot,
    Kilometres Per Hour,
    Degrees Celcius,
    Centipoise,
    Voltage,
    Ampere,
    TEU,
    FFE,
    ......</xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="UnitOfMeasurementId"
                      type="AMM:Integer13Type"
                      minOccurs="0"
                      maxOccurs="1"/>
         <xsd:element name="UnitOfMeasurementName"
                      type="AMM:String100Type"
                      minOccurs="0"
                      maxOccurs="1"/>
         <xsd:element name="UnitOfMeasurementAbbreviationVal"
                      type="AMM:String1000Type"
                      minOccurs="0"
                      maxOccurs="1"/>
         <xsd:element name="UnitOfMeasurementDesc"
                      type="AMM:String250Type"
                      minOccurs="0"
                      maxOccurs="1"/>
         <xsd:element name="UnitOfMeasurementSymbolVal"
                      type="AMM:String1000Type"
                      minOccurs="0"
                      maxOccurs="1"/>
         <xsd:element name="MeasurementSystemCd"
                      type="AMM:String50NonNullType"
                      minOccurs="0"
                      maxOccurs="1"/>
      </xsd:sequence>
   </xsd:complexType>
   <xsd:complexType name="TelecommunicationsNumberStructure">
      <xsd:annotation>
         <xsd:documentation>A number that can be used for contact via a telecommunications system. Examples include:-
 * Phone number (land line)
 * Mobile or cell phone number
* Fax number
* Pager number</xsd:documentation>
      </xsd:annotation>
      <xsd:complexContent>
		<xsd:extension base="ContactAddressStructure">
		<xsd:sequence>
		         <xsd:element name="TelecommunicationsNumberIdentifier"
		                      type="AMM:Integer13Type"
		                      minOccurs="0"
		                      maxOccurs="1"/>
		         <xsd:element name="CountryCodeNumber"
		                      type="AMM:String50NonNullType"
		                      minOccurs="0"
		                      maxOccurs="1"/>
		         <xsd:element name="AreaCodeNumber"
		                      type="AMM:String50NonNullType"
		                      minOccurs="0"
		                      maxOccurs="1">
		            <xsd:annotation>
		               <xsd:documentation>A set of digits in a telephone number
											that represent the area code. The
											number of digits may vary by
											country.
						</xsd:documentation>
		            </xsd:annotation>
		         </xsd:element>
		         <xsd:element name="TelephoneNumber"
		                      type="AMM:String50NonNullType"
		                      minOccurs="0"
		                      maxOccurs="1">
		         </xsd:element>
		         <xsd:element name="ExtensionNumber"
		                      type="AMM:String50NonNullType"
		                      minOccurs="0"
		                      maxOccurs="1">
		            <xsd:annotation>
		               <xsd:documentation>The extension for the phone. This
		may be null if no extension applies
		to this number.</xsd:documentation>
		            </xsd:annotation>
		         </xsd:element>
		         <xsd:element name="ExchangeNumber"
		                      type="AMM:String50NonNullType"
		                      minOccurs="0"
		                      maxOccurs="1">
		            <xsd:annotation>
		               <xsd:documentation>A set of digits in a telephone number
		that represent a central office
		exchange. The number of digits may
		vary by country.</xsd:documentation>
		            </xsd:annotation>
		         </xsd:element>
		         <xsd:element name="IsHomeNumberIndicator"
		                      type="AMM:BooleanType"
		                      minOccurs="0"
		                      maxOccurs="1">
		            <xsd:annotation>
		               <xsd:documentation>Indicator for whether the Telephone
		is a Home number.</xsd:documentation>
		            </xsd:annotation>
		         </xsd:element>
		         <xsd:element name="LineNumber"
		                      type="AMM:String50NonNullType"
		                      minOccurs="0"
		                      maxOccurs="1">
		            <xsd:annotation>
		               <xsd:documentation>A set of digits in a telephone number
		that represent the line number from
		the central office. The number of
		digits may vary by country.</xsd:documentation>
		            </xsd:annotation>
		         </xsd:element>
		         <xsd:element name="IsWireLineIndicator"
		                      type="AMM:BooleanType"
		                      minOccurs="0"
		                      maxOccurs="1">
		            <xsd:annotation>
		               <xsd:documentation>elements of a ported number no
		longer identify an area or an
		exchange.</xsd:documentation>
		            </xsd:annotation>
		         </xsd:element>
		         <xsd:element name="InOfficeHoursIndicator"
		                      type="AMM:BooleanType"
		                      minOccurs="0"
		                      maxOccurs="1"/>
		                  <xsd:element name="TelecommunicationNumberSubTypeCd"
		                      type="TelecommunicationNumberSubTypeCdStructure"
		                      minOccurs="0"
		                      maxOccurs="1">
		                      <xsd:annotation>
			               <xsd:documentation>
			               	             A code classifying a telecommunication number. 
										Examples include:-
										* Phone number (land line)
										* Mobile or cell phone number
										* Fax number
										* Pager number
			               </xsd:documentation>
		            </xsd:annotation>
		         </xsd:element>
		         <!-- This is a Non ELDM Aligned Field that is requested by the project -->
		         <xsd:element name="Country" type="CountryStructure" minOccurs="0" maxOccurs="1" />
       </xsd:sequence>
       </xsd:extension>
       </xsd:complexContent>
   </xsd:complexType>
   
   <xsd:simpleType name="TelecommunicationNumberSubTypeCdStructure">
    <xsd:restriction base="AMM:String3NonNullType">
        <xsd:enumeration value="TEL" />
        <xsd:enumeration value="MOB" />
        <xsd:enumeration value="FAX" />
        <xsd:enumeration value="PAG" />
    </xsd:restriction>
	</xsd:simpleType>
   

			<xsd:element name="AddressMailabiltyScore" type="AMM:String10Type"
				minOccurs="0" maxOccurs="1" />
			<xsd:element name="AddressMatchScore" type="AMM:String10Type"
				minOccurs="0" maxOccurs="1" />
		</xsd:sequence>
	</xsd:complexType>


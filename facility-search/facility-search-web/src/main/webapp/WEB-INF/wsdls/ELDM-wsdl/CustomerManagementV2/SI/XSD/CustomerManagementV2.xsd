		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:pattern value="([a-zA-Z0-9.,:;&quot;$%^&amp;*\-&apos;( )/]+){3,}"/>
			<xsd:maxLength value="128"/>
		</xsd:restriction>
	</xsd:simpleType>
	
	<xsd:element name="StreetName">
		<xsd:annotation>
			<xsd:documentation>
				Streetname to be used in address type and
				searchCustomer
			</xsd:documentation>
		</xsd:annotation>
		<xsd:simpleType>
			<xsd:restriction base="xsd:string">
				<xsd:maxLength value="45" />
			</xsd:restriction>
		</xsd:simpleType>
	</xsd:element>
	
	<xsd:element name="PostalCode">
		<xsd:annotation>
			<xsd:documentation>
				PostalCode to be used in address type and
				searchCustomer
			</xsd:documentation>
		</xsd:annotation>
		<xsd:simpleType>
			<xsd:restriction base="xsd:string">
				<xsd:maxLength value="10" />
			</xsd:restriction>
		</xsd:simpleType>
	</xsd:element>
	
	<xsd:element name="City" type="StringMinLen1">
		<xsd:annotation>
			<xsd:documentation>
				City to be used in address type and searchCustomer
			</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	
	<xsd:simpleType name="TaxNumberTypeEnum">
		<xsd:annotation>
			<xsd:documentation>
				TAXNO1 Tax Number 1 TAXNO2 Tax Number 2 TAXNO3 Tax Number 3 TAXNO4 Tax Number 4 VATALT Foreign VAT Reg. Number VATREG VAT Registration Number. ALTREG Alternate Registration Number
			</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="TAXNO1"/>
			<xsd:enumeration value="TAXNO2"/>
			<xsd:enumeration value="TAXNO3"/>
			<xsd:enumeration value="TAXNO4"/>
			<xsd:enumeration value="VATALT"/>
			<xsd:enumeration value="VATREG"/>
			<xsd:enumeration value="ALTREG"/>
		</xsd:restriction>
	</xsd:simpleType>
	
	<xsd:simpleType name="ReferenceTypeEnum">
    <xsd:annotation>
      <xsd:documentation> EOR EORI Economic Operator Identification and Registration DOD DODAC_ID US
        Government Location code SAN Shipping Association Number Shipping Association Number COR
        Customer Own Reference Customer's Own Reference FOF FOFMCD_ID Federal Maritime Commission
        Code SIR SIREN SIREN APM AMPS Trading Partner ID AMPS Trading Partner ID BVD BvD ID Number
        Bureau van Dijk Identification Number DUN D.U.N.S Dun &amp; Bradstreet Identification Number
        BRN Business Registration number Business Registeration Number TCO Tax Collection Office
        Name Tax Collection Office Name CUS Customer Reference Customer Reference
      </xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="EOR"/>
      <xsd:enumeration value="DOD"/>
      <xsd:enumeration value="SAN"/>
      <xsd:enumeration value="COR"/>
      <xsd:enumeration value="FOF"/>
      <xsd:enumeration value="SIR"/>
      <xsd:enumeration value="APM"/>
      <xsd:enumeration value="BVD"/>
      <xsd:enumeration value="DUN"/>
      <xsd:enumeration value="BRN"/>
      <xsd:enumeration value="TCO"/>
      <xsd:enumeration value="CUS"/>
      <xsd:enumeration value="BVD_GUO"/>
      <xsd:enumeration value="BVD_HQ"/>
      <xsd:enumeration value="ED1"/>
      <xsd:enumeration value="ED2"/>
      <xsd:enumeration value="ED3"/>
      <xsd:enumeration value="SFDC"/>
      <!--xsd:enumeration value="CUSTOMS_ID" /> -->
    </xsd:restriction>
  </xsd:simpleType>
	
	<xsd:simpleType name="OperatorTypeEnum">
    	<xsd:restriction base="xsd:string">
      		<xsd:enumeration value="MAEU"> <xsd:annotation><xsd:appinfo>MAERSK LINES</xsd:appinfo></xsd:annotation></xsd:enumeration>
      		<xsd:enumeration value="SAFM"> <xsd:annotation><xsd:appinfo>SAFMARINE</xsd:appinfo></xsd:annotation></xsd:enumeration>
      		<xsd:enumeration value="SEJJ"> <xsd:annotation><xsd:appinfo>SEAGO LINE</xsd:appinfo></xsd:annotation></xsd:enumeration>
      		<xsd:enumeration value="SEAU"> <xsd:annotation><xsd:appinfo>SEA-LAND SERVICE</xsd:appinfo></xsd:annotation></xsd:enumeration>
      		<xsd:enumeration value="MCCQ"> <xsd:annotation><xsd:appinfo>MCC TRANSPORT</xsd:appinfo></xsd:annotation></xsd:enumeration>
			<xsd:enumeration value="MCPU"> <xsd:annotation><xsd:appinfo>MCCQ REPLACEMENT</xsd:appinfo></xsd:annotation></xsd:enumeration>
    	</xsd:restriction>
  </xsd:simpleType>
	<xsd:simpleType name="PhoneNumberTypeEnum">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="MOB" />
			<xsd:enumeration value="TEL" />
			<xsd:enumeration value="FAX" />
		</xsd:restriction>
	</xsd:simpleType>
	
	<xsd:simpleType name="SegmentClassificationString">
		<xsd:annotation>
			<xsd:documentation>Trading name validation </xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:pattern value="([a-zA-Z0-9]+){1,4}"/>
		</xsd:restriction>
	</xsd:simpleType>

	<xsd:simpleType name="SegmentClassificationValueString">
		<xsd:annotation>
			<xsd:documentation>Trading name validation </xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:pattern value="([a-zA-Z0-9]+){1,4}"/>
		</xsd:restriction>
	</xsd:simpleType>
	
	<xsd:element name="DeleteFlag" type="xsd:boolean">
		<xsd:annotation>
			<xsd:documentation>
				Indicator if an entity or a relation should be
				deleted (DeleteFlag = 1).
				Will set the entity or relationship to
				inactive in the MDM-Hub.
				Only to be used if there is no separate flag
				to set the status of the
				entity.
			</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:simpleType name="StringMinLen1">
		<xsd:annotation>
			<xsd:documentation>Values of the match score of Address Doctor
			</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:pattern value="(.+){1,}" />
			<xsd:maxLength value="35" />
		</xsd:restriction>  
	</xsd:simpleType>
	
	<xsd:simpleType name="ISOCountryCode">  
		<xsd:annotation>
			<xsd:documentation>Country code in ISO 2-digit format
			</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">  
			<xsd:pattern value="[A-Z]{2}" />
		</xsd:restriction>
	</xsd:simpleType>
	
	<!-- RetrieveTaxTypeLocalNameRequest- -->
  
	<xsd:element name="RetrieveTaxTypeLocalNameRequest">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element name="Country" type="ISOCountryStructure" />
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	
	<xsd:element name="RetrieveTaxTypeLocalNameResponse">
		<xsd:complexType>
			<xsd:annotation>
				<xsd:documentation>
					Response for retrieve tax type local name
				</xsd:documentation>
			</xsd:annotation>
			<xsd:sequence>
				<xsd:element name="TaxType" type="TaxLocalNameType"
					minOccurs="0" maxOccurs="unbounded" />
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	
	
	
	<xsd:complexType name="TaxLocalNameType">
		<xsd:annotation>
			<xsd:documentation> Country specific tax number(s) per customer
			</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>			
			<xsd:element name="TaxType" type="TaxNumberTypeEnum" />
			<xsd:element name="ISOCountryCode" type="ISOCountryCode" />
			<xsd:element name="TaxTypeLocalName" type="AMM:String100Type" maxOccurs="1" minOccurs="0"/>			 
			<xsd:element ref="DeleteFlag" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Set to 1 to completly delete the tax number
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:element name="Contact" type="RetrieveCustomerContactResponseType" />
	<xsd:element name="Customer">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element name="CustomerDetails" minOccurs="1" maxOccurs="1" type="RetrieveCustomerResponseType" />
				<xsd:element  minOccurs="1" maxOccurs="1"
					ref="RetrieveRelationshipsResponse" />
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="Concern">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element name="ConcernCodeType" minOccurs="1" maxOccurs="1" type="ConcernCodeType" />
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
</xsd:schema>

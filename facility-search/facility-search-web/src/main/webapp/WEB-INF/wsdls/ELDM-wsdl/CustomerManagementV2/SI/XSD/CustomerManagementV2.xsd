	<xsd:complexType name="CustomerAddressStructure">
		<xsd:sequence>
			<xsd:element name="PostalAddressDetailHouseNum" type="AMM:String50Type" minOccurs="0"/>
			<xsd:element name="PostalAddressDetailStreetName" type="AMM:String100Type" minOccurs="0"/>
			<xsd:element name="PostalAddressDetailMailBoxNum" type="AMM:String50Type" minOccurs="0"/>
			<xsd:element name="PostalAddressDetailBuildingNum" type="AMM:String50Type" minOccurs="0"/>
			<xsd:element name="Suburb" type="AMM:String100Type" minOccurs="0"/>
			<xsd:element name="District" type="AMM:String100Type" minOccurs="0"/>
			<xsd:element name="City" type="AMM:String100NonNullType"/>
			<xsd:element name="Region" type="AMM:String100Type" minOccurs="0"/>
			<xsd:element name="ISOCountry" type="ISOCountryStructure"/>
			<xsd:element name="PostalCd" type="AMM:String50Type" minOccurs="0"/>
			<xsd:element name="TaxJurisdictionCode" type="AMM:Integer10Type" minOccurs="0"/>
			<xsd:element name="AddressAuditData" type="CustomerAuditDataStructure" minOccurs="0"/>
			<xsd:element name="AddressWorkflowIndicator" type="AMM:String50Type" minOccurs="0"/>
			<xsd:element name="AddressMailabilityScore" type="AddressMailabilityScoreType" minOccurs="0"/>
      		<xsd:element name="AddressMatchScore" type="AddressMatchScoreType" minOccurs="0"/>
		</xsd:sequence>
		<xsd:attribute name="Language" type="ISOLanguageCode" default="en">
      <xsd:annotation>
        <xsd:documentation> Attribute for setting the language of language specific entities
          (primarily address fields) Defaults to english </xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
	</xsd:complexType>
	<xsd:simpleType name="ISOLanguageCode">
		<xsd:annotation>
			<xsd:documentation>Language code in ISO 2-digit format (ISO 639-1) </xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:pattern value="[a-zA-Z0-9]{2}"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="AddressMailabilityScoreType">
		<xsd:restriction base="xsd:int">
			<xsd:minInclusive value="0"/>
			<xsd:maxInclusive value="5"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="AddressMatchScoreType">
		<xsd:restriction base="xsd:string">
			<xsd:pattern value="[A-Z][A-Z0-9]"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:complexType name="CustomerAuditDataStructure">
		<xsd:sequence>
			<xsd:element name="CreationDate" type="AMM:DateTimeType"/>
